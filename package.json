    "express": "5.2.1",
    "lottie-react": "2.4.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "5.5.0",
    "react-router-dom": "7.13.0",
    "react-toastify": "11.0.5",
    "recharts": "2.15.4",
    "tailwind-merge": "3.5.0",
